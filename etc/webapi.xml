<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/lb-document-generator/invoice-shipment/:orderNumber" method="POST">
        <service class="FossilEcommerce\LBDocumentGenerator\Api\InvoiceRepositoryInterface" method="getInvoiceShipmentJson"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>

    <route url="/V1/lb-document-generator/return/:orderNumber" method="POST">
        <service class="FossilEcommerce\LBDocumentGenerator\Api\ReturnRepositoryInterface" method="getReturnJson"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>

</routes>